/* components/video-header/video-header.wxss */
.nav-header {
  margin: 16rpx 0;
  padding: 0 10rpx;
  display: flex;
}
.nav-header .left,
.nav-header .right {
  width: 40rpx;
  height: 40rpx;
}
.nav-header .center {
  margin: 0 15rpx;
  flex: 1;
  text-align: center;
  font-size: 20rpx;
  line-height: 40rpx;
  color: red;
  border: 2rpx solid #cccccc;
  border-radius: 10rpx;
}
